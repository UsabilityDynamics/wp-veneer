{
  "name": "usabilitydynamics/wp-veneer",
  "version": "0.1.0",
  "homepage": "https://github.com/UsabilityDynamics/wp-veneer",
  "keywords": [
    "wordpress",
    "multisite",
    "varnish"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Andy Potanin",
      "homepage": "http://usabilitydynamics.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "require": {
    "php": ">=5.3",
    "usabilitydynamics/lib-wp-xmlrpc": "master",
    "usabilitydynamics/lib-settings": "master",
    "usabilitydynamics/lib-ui": "master",
    "usabilitydynamics/lib-utility": "master"
  },
  "autoload": {
    "classmap": [
      "lib/class-bootstrap.php",
      "lib/class-cache.php",
      "lib/class-documents.php",
      "lib/class-image.php",
      "lib/class-log.php",
      "lib/class-media.php",
      "lib/class-security.php",
      "lib/class-varnish.php"
    ]
  }
}
