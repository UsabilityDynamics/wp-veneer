{
  "name": "usabilitydynamics/wp-veneer",
  "version": "0.5.1",
  "homepage": "https://github.com/UsabilityDynamics/wp-veneer",
  "keywords": [
    "wordpress",
    "multisite",
    "varnish"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Andy Potanin",
      "homepage": "http://usabilitydynamics.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "require": {
    "php": ">=5.3",
    "usabilitydynamics/lib-wp-rpc": "master",
    "usabilitydynamics/lib-settings": "master",
    "usabilitydynamics/lib-ui": "master",
    "usabilitydynamics/lib-utility": "master",
    "aws/aws-sdk-php": "2.5.*@dev",
    "matthiasmullie/minify": "1.2.0",
    "mrclay/minify": "master"
  },
  "autoload": {
    "classmap": [ 
      "lib/class-bootstrap.php",
      "lib/class-w3.php",
      "lib/class-cache.php",
      "lib/class-documents.php",
      "lib/class-image.php",
      "lib/class-log.php",
      "lib/class-media.php",
      "lib/class-security.php",
      "lib/class-varnish.php",
      "lib/class-search.php"  
     ]
  }
}
